    "editor.findRangeHighlightBackground": "#e0525255",

    "editor.hoverHighlightBackground": "#3399ff55",

    "editor.wordHighlightBackground": "#5cd69955",
    "editor.wordHighlightStrongBackground": "#e0525255",
