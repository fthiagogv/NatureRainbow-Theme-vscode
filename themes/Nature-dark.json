  "tokenColors": [
    {
      "name": "Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#d27b53",
        "foreground": "#1b1b1bFFF"
      }
    },
    {
      "name": "WARNING",
      "scope": "markup.changed.diff,token.warn-token",
      "settings": {
        "foreground": "#d6d65c"
      }
    },
    {
      "name": "INFO",
      "scope": "meta.diff.header.from-file,punctuation.definition.from-file.diff",
      "settings": {
        "foreground": "#3399ff"
      }
    },
    {
      "name": "ADDED",
      "scope": "markup.inserted.diff",
      "settings": {
        "foreground": "#5cd699"
      }
    },
    {
      "name": "ERROR",
      "scope": "markup.deleted.diff,invalid.illegal,invalid.broken,invalid.illegal.non-null-typehinted.php,token.error-token",
      "settings": {
        "foreground": "#e05252"
      }
    },
    {
      "name": "Red Collor",
      "scope": "support.type.object.console, source.java,meta.definition.variable.name.java, meta.function-call.arguments.python,meta.function-call.python,entity.name.type",
      "settings": {
        "foreground": "#f78f8f"
